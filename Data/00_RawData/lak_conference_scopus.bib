@CONFERENCE{Klebanov2020490,
author={Klebanov, B.B. and Loukina, A. and Lockwood, J. and Liceralde, V.R.T. and Sabatini, J. and Madnani, N. and Gyawali, B. and Wang, Z. and Lentini, J.},
title={Detecting learning in noisy data: The case of oral reading fluency},
journal={ACM International Conference Proceeding Series},
year={2020},
