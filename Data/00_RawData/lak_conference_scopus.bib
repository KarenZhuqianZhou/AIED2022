@CONFERENCE{Hilliger2020181,
author={Hilliger, I. and Aguirre, C. and Miranda, C. and Celis, S. and Pérez-Sanagustín, M.},
title={Design of a curriculum analytics tool to support continuous improvement processes in higher education},
journal={ACM International Conference Proceeding Series},
year={2020},
