@ARTICLE{Kuncheva2017139,
author={Kuncheva, Z. and Montana, G.},
title={Multi-scale community detection in temporal networks using spectral graph wavelets},
journal={Lecture Notes in Computer Science (including subseries Lecture Notes in Artificial Intelligence and Lecture Notes in Bioinformatics)},
year={2017},
volume={10708 LNCS},
pages={139-154},
doi={10.1007/978-3-319-71970-2_12},
