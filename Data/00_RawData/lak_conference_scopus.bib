@CONFERENCE{Motz2019300,
author={Motz, B. and Quick, J. and Schroeder, N. and Zook, J. and Gunkel, M.},
title={The validity and utility of activity logs as a measure of student engagement},
journal={ACM International Conference Proceeding Series},
year={2019},
pages={300-309},
doi={10.1145/3303772.3303789},
note={cited By 4},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85062785253&doi=10.1145%2f3303772.3303789&partnerID=40&md5=f2b9bc7064d560f253791719c9d7150e},
affiliation={Department of Psychological and Brain Sciences, Indiana University, Indiana, United States; eLearning Design and Services, Indiana University, Indiana, United States},
abstract={Learning management system (LMS) web logs provide granular, near-real-time records of student behavior as learners interact with online course materials in digital learning environments. However, it remains unclear whether LMS activity indeed reflects behavioral properties of student engagement, and it also remains unclear how to deal with variability in LMS usage across a diversity of courses. In this study, we evaluate whether instructors' subjective ratings of their students' engagement are related to features of LMS activity for 9,021 students enrolled in 473 for-credit courses. We find that estimators derived from LMS web logs are closely related to instructor ratings of engagement, however, we also observe that there is not a single generic relationship between activity and engagement, and what constitutes the behavioral components of “engagement” will be contingent on course structure. However, for many of these courses, modeled engagement scores are comparable to instructors' ratings in their sensitivity for predicting academic performance. As long as they are tuned to the differences between courses, activity indices from LMS web logs can provide a valid and useful proxy measure of student engagement. © 2019 Copyright is held by the owner/author(s). Publication rights licensed to ACM.},
author_keywords={LMS;  Student engagement;  Trace data;  Web logs},
document_type={Conference Paper},
source={Scopus},
}

@CONFERENCE{Choi2019160,
author={Choi, H. and Brooks, C. and Dowell, N. and Teasley, S.},
title={Social comparison in MOOCs: Perceived SES, opinion, and message formality},
journal={ACM International Conference Proceeding Series},
year={2019},
pages={160-169},
doi={10.1145/3303772.3303773},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85062783523&doi=10.1145%2f3303772.3303773&partnerID=40&md5=937800944f65ead27e0f81d2c117bd96},
affiliation={University of Michigan, Ann Arbor, MI, United States},
abstract={There has been limited research on how perceptions of socioeconomic status (SES) and opinion difference could influence peer feedback in Massive Open Online Courses (MOOCs). Using social comparison theory [12], we investigated the influence of ability and opinion-related factors on peer feedback text in a data science MOOC. Perceived SES of peers and the formality of written responses were used as the ability-related factor, while agreement between learners represented the opinion-related factor. We focused on understanding the behaviors of those learners who are most prevalent in MOOCs; those from high socioeconomic countries. Through two studies, we found a strong and repeated influence of agreement on affect and formality in feedback to peers. While a mediation effect of perceived SES was found, a significant effect of formality was not. This work contributes to an understanding of how social comparison theory can be operationalized in online peer writing environments. © 2019 Copyright held by the owner/author(s). Publication rights licensed to ACM.},
author_keywords={MOOC;  Peer feedback;  Social comparison},
document_type={Conference Paper},
source={Scopus},
}

