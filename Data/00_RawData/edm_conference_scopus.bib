title={A promising classification method for predicting distance students’ performance},
journal={Proceedings of the 5th International Conference on Educational Data Mining, EDM 2012},
year={2012},
note={cited By 8},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85084012913&partnerID=40&md5=fe496070df01141253af84fa2acfaefa},
affiliation={Universidad de Cantabria, Avda. Los Castros s/n, Santander, Spain},
abstract={Predicting the students’ performance is still a challenging task despite being one of the oldest and most popular applications of data mining in education. One of the problems encountered when analyzing data from e-learning platforms is that it presents statistical outliers as a consequence of how students work in online courses. It causes that classifiers are built with less accuracy than desired. To solve this problem we propose a new method to eliminate outliers as previous step to build the classifier. In this work we describe our meta-algorithm and compare its performance with respect to several well-known classification techniques. The comparison is evaluated in terms of accuracy, true positive and true negative rate. The results obtained shows that our approach produces more accurate models. © 2012 www.educationaldatamining.org. All rights reserved.},
document_type={Conference Paper},
source={Scopus},
}

@CONFERENCE{López2012,
author={López, M.I. and Luna, J.M. and Romero, C. and Ventura, S.},
title={Classification via clustering for predicting final marks based on student participation in forums},
journal={Proceedings of the 5th International Conference on Educational Data Mining, EDM 2012},
year={2012},
note={cited By 76},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85084012504&partnerID=40&md5=e4cf97dcb27bce63a54b51018d76948f},
affiliation={Department of Computer Science and Numerical Analysis, University of Córdoba, Córdoba, Spain},
abstract={This paper proposes a classification via clustering approach to predict the final marks in a university course on the basis of forum data. The objective is twofold: to determine if student participation in the course forum can be a good predictor of the final marks for the course and to examine whether the proposed classification via clustering approach can obtain similar accuracy to traditional classification algorithms. Experiments were carried out using real data from first-year university students. Several clustering algorithms using the proposed approach were compared with traditional classification algorithms in predicting whether students pass or fail the course on the basis of their Moodle forum usage data. The results show that the Expectation-Maximisation (EM) clustering algorithm yields results similar to those of the best classification algorithms, especially when using only a group of selected attributes. Finally, the centroids of the EM clusters are described to show the relationship between the two clusters and the two classes of students. © 2012 www.educationaldatamining.org. All rights reserved.},
author_keywords={Classification;  Classification via clustering;  Forums;  Prediction;  Social networks analysis},
document_type={Conference Paper},
source={Scopus},
}

@CONFERENCE{Rau2012,
