@CONFERENCE{Rus2016496,
author={Rus, V. and Gautam, D. and Swiecki, Z. and Shaffer, D.W. and Graesser, A.C.},
title={Assessing student-generated design justifications in engineering virtual internships},
journal={Proceedings of the 9th International Conference on Educational Data Mining, EDM 2016},
