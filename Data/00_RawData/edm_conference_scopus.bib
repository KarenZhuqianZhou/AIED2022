@CONFERENCE{Oeda2013,
author={Oeda, S. and Yamanishi, K.},
title={Extracting time-evolving latent skills from examination time series},
journal={Proceedings of the 6th International Conference on Educational Data Mining, EDM 2013},
year={2013},
note={cited By 1},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85084011844&partnerID=40&md5=a81959bde2e9907e28e184f22cec3644},
affiliation={Department of Information and Computer Engineering, Kisarazu National College of Technology, 2–11–1 Kiyomidai, Kisarazu-shi, Chiba, 292–0041, Japan; Department of Mathematical Informatics, University of Tokyo, 7–3–1 Hongo, Bunkyo-ku, Tokyo, 113–8656, Japan},
abstract={Examination results are used to judge whether an examinee possesses the desired latent skills. In order to grasp the skills, it is important to find which skills a question item contains. The relationship between items and skills may be represented by what we call a Q-matrix. Recent studies have been attempting to extract a Q-matrix with non-negative matrix factorization (NMF) from a set of examinees’ test scores. However, they did not consider the time-evloving nature of latent skills. In order to comprehend the learning effects in the educational process, it is significant to study how the distribution of examinees’ latent skills changes over time. In this paper, we propose novel methods for extracting both a Q-matrix and time-evolving latent skills from examination time series, simultaneously. © 2013 International Educational Data Mining Society. All rights reserved.},
document_type={Conference Paper},
source={Scopus},
}

@CONFERENCE{Ramachandran2013,
author={Ramachandran, L. and Ravindran, B. and Gehringer, E.F.},
title={Determining review coverage by extracting topic sentences using a graph-based clustering approach},
