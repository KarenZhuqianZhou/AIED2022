@CONFERENCE{al-Rifaie2016264,
author={al-Rifaie, M.M. and Yee-King, M. and d’Inverno, M.},
title={Investigating swarm intelligence for performance prediction∗},
journal={Proceedings of the 9th International Conference on Educational Data Mining, EDM 2016},
