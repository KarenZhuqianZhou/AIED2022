}

@CONFERENCE{Yeung2019683,
author={Yeung, C.-K.},
title={Deep-IRT: Make deep learning based knowledge tracing explainable using item response theory},
journal={EDM 2019 - Proceedings of the 12th International Conference on Educational Data Mining},
