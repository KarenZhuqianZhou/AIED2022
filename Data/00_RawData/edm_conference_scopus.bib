@CONFERENCE{Hicks201678,
author={Hicks, D. and Liu, Z. and Eagle, M. and Barnes, T.},
title={Measuring gameplay affordances of user-generated content in an educational game},
journal={Proceedings of the 9th International Conference on Educational Data Mining, EDM 2016},
year={2016},
pages={78-85},
note={cited By 4},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85056148739&partnerID=40&md5=8800cf031ede5d03cf16f4082be54190},
affiliation={North Carolina State University, 911 Oval Drive, Raleigh, NC  27606, United States; Carnegie-Mellon University, 5000 Forbes Avenue, Pittsburgh, PA  15213, United States},
abstract={Level creation is a creative game-play exercise that resembles problem-posing, and has shown to be engaging and helpful for players to learn about the game’s core mechanic. However, in user-authoring environments, users often create levels without considering the game’s objective, or with entirely different objectives in mind, resulting in levels which fail to afford the core gameplay mechanic. This poses a bigger threat to educational games, because the core gameplay is aligned with the learning objectives. Therefore, such levels fail to provide any opportunity for players to practice the skills the game is designed to teach. To address this problem, we designed and compared three versions of level creators in a programming game – Freeform, Programming, and Building-Block. Our results show that a simple-to-use building-block editor can guarantee levels that contain some affordances, but an editor designed to use the same core mechanic as gameplay results in the highest-quality levels. © 2016 International Educational Data Mining Society. All rights reserved.},
author_keywords={Educational data mining;  Educational game;  Learning analytics;  User-created content},
document_type={Conference Paper},
source={Scopus},
}

@CONFERENCE{Huang2016593,
author={Huang, Y. and Guerra-Hollstein, J.D. and Brusilovsky, P.},
